<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Include all high and medium priority bugs -->
    <Match>
        <Bug pattern="*"/>
    </Match>
    
    <!-- Exclude test classes from certain checks -->
    <Match>
        <Class name="~.*Test.*"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
</FindBugsFilter>